{
  "name": "@junicus_/react-msal-redux",
  "version": "0.1.15",
  "description": "React library that supports authenticating against Azure AD using Msal and Redux",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "repository": "github:Junicus/react-msal-redux",
  "author": "Nelson Segarra <junicus@outlook.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf build",
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "yarn clean && yarn build",
    "prepublishOnly": "yarn test && yarn lint",
    "preversion": "yarn lint",
    "version": "yarn format && git add src",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "jest": "^24.7.1",
    "prettier": "^1.17.0",
    "redux": "^4.0.1",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.3"
  },
  "keywords": [
    "Msal",
    "React",
    "Redux"
  ],
  "files": [
    "build/**/*"
  ],
  "dependencies": {
    "msal": "^1.0.2"
  }
}
